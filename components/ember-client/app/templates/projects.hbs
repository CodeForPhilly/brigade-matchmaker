
<div class="container-fluid">
  <h2>Projects</h2>
  
  {{#each model as |project|}}
    <hr/>

    <article class="row account">
      
      <div class="col-1">
        {{project.id}}
      </div>
      
      <div class="col-2">
        <h3>{{project.name}}</h3>
      </div>
      
      <div class="col-4 detail">
        {{project.description}}
      </div>
      
    </article>
    
  {{/each}}

</div>

{{outlet}}
